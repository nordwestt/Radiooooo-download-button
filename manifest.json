{
    "name":"Radiooooo download button",
    "description":"Adds a download button to Radiooooo",
    "version": "1.0",
    "action":{},
    "manifest_version":3,
    "permissions":[],
    "commands":{
    },
    "content_scripts": [
        {
          "js": [
            "content.js"
          ],
          "matches": [
            "http://radiooooo.com/*",
            "https://radiooooo.com/*"
          ]
        }
    ], 
    "icons":{
        "16":"icon_16.png",
        "48":"icon_48.png",
        "128":"icon_128.png"
    }
}
